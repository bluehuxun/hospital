page{
  background: #F1F1F1;
}
.search{
  width:14px;
}
.search_wrap{
  width:30px;
  background:#fff;
  display:flex;
  height:40px;
  align-items:center;
  justify-content:center;
  border-top-left-radius : 4px;
  border-bottom-left-radius : 4px;  
}
.search_subject_wrap{
  border-radius: 10px;
  padding: 10px 0 10px 10px;
}
.search_subject{
  width: calc(100% - 40px);
  background: #FFFFFF;
  height:40px;
  border-top-right-radius : 4px;
  border-bottom-right-radius : 4px;
}
.subjects>view>view{
  line-height:40px;
  padding: 0 0 0 6px;
}
.small_subject{
  width:100%;
  background: #fff;
}
.small_subject view{
  color:#333333;
}
.subjects .show{
  display: block;
}
.small_subject .subject_con view navigator{
  background: #fff;
  border-bottom: 1px solid #EEEEEE;
  line-height:40px;
  padding: 0 6px;
  display:flex;
  align-items:center;
  align-content:space-around;
  justify-content:space-between;
}
.subject_sel{
  width:10px;
}
.empty_result{
  padding: 30% 0 0 0;
  color:#b9b9b9;
}