page{
    font-size:32rpx;
}
.flex_center{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.show_face_recog_lead{
    flex: 1;
    text-align: right;
    justify-content: right;
    color: #1abb85;
}
.close_pop{
    width: calc(100% - 40rpx);
    height: 40px;
    line-height: 40px;
    margin: 10px auto;
    background: #1abb85;
    color:#fff;
}
checkbox{
    width: 20px;
    height: 20px;
}
.avatar{
  width:100rpx;
}
.mine_items{
    border-bottom:4rpx solid #F2F2F2;
    color:#272727;
    line-height: 80rpx;
}
.mine_items text{
  color:#666666;
}
.mine_item{
  border-top:2rpx solid #F2F2F2;
  min-height:80rpx;
  justify-content:space-between;
  padding:20rpx 30rpx;
  position: relative;
}
.last_mine_item{
  border-bottom:2rpx solid #F2F2F2;
}
.mine_icon{
  width:50rpx;
  margin:0 30rpx 0 0;
}
.nav{
  width:20rpx;
}
.id_card{
  border-bottom:20rpx solid #F2F2F2;
}
.mine_item view:first-child{
  width:160rpx;
    height:60rpx;
    line-height:60rpx;
}
.info_right{
  width:calc(100% - 160rpx);
}
.info_right input{
    height: 60rpx;
    line-height: 60rpx;
}
.get_phone{
    min-width:280rpx;
}
.get_code{
  width: 300rpx;
  position:absolute;
  z-index: 1;
  right:4rpx;
  top:11rpx;
}
.agree{
  padding: 10rpx 0 0 30rpx;
}
.submit{
    margin: 80rpx 0 0 0;
    line-height:80rpx;
}
.agree_title{
    padding: 0 0 0 20rpx;
}
.agree_con{
    height: 20px;
    line-height: 20px;
}

/*协议弹出框*/
.agree_con_info{
    width: 100%;
    height: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    position: fixed;
    z-index: 1;
    top:0;
    right: 0;
    bottom:0;
    left:0;
    background: #fff;
}
.confirm_wrap{
    padding: 10px;
}
.appoint_con{
    padding: 20rpx 0 0 0;
}
.appoint_con>view{
    padding: 0 20rpx;
}
.confirm{
    height:80rpx;
    line-height:80rpx;
    background: #1abb85;
    color:#fff;
}

.face_recog_lead_wrap{
    position: fixed;
    z-index: 1;
    width: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    background: #fff;
    top:0;
    right:0;
    bottom:0;
    left:0;
}
.face_recog_lead_view{
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}
.face_recog_lead{
    width: 100%;
}