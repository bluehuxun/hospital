page{
    background: #fff;
    line-height: 80rpx;
}
.header{
  line-height:80rpx;
  display:flex;
  align-items:center;
  justify-content:flex-end;
  padding: 0 18rpx 0 0;
}
.set_time{
  float:right;
  padding:0 10rpx;
}
.flex{
  justify-content: space-between;
}
.report_item{
  background: #fff;
  margin-bottom:20rpx;
}
.title{
  padding: 0 18rpx;
  line-height:80rpx;
  border-bottom:2rpx solid #dedede;
}
.nav{
  width:16rpx;
}
.flex .title_left{
  flex:none;
}
.text-right{
  text-align: right;
}
.report_info{
  padding: 0 0 0 18rpx;
}
.report_info view{
  display: flex;
  align-items: center;
  line-height:80rpx;
}
.exma_icon{
  width:30rpx;
  margin:0 10rpx 0 0;
}
.triangle{
  width:30rpx;
}

.list>view>view:nth-child(1){
    width:40%;
}
.list>view>view:nth-child(2){
    width:20%;
}
.list>view>view:nth-child(3){
    width:40%;
}
.page_list>view>view{
    padding: 0 20rpx;
    border-bottom: 2rpx solid #f2f2f2;
}