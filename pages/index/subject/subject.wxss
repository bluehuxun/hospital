page{
  height: 100%;
  background: #F1F1F1;
}
.header_nav{
  height:60px;
  padding:20rpx 20rpx;
  display:flex;
  background:#fff;
  align-item:center;
  box-sizing: border-box;
}
.header_nav_item{
  display:flex;
  align-items:center;
}
.flex1{
  flex:1;
}
.nav{
  width:24rpx;
  /* position: relative;
  top:20rpx; */
}
.time_nav{
  width:14rpx;
  transform:rotate(90deg);
  margin:0 0 0 20rpx;
}
.avatar{
  width:80rpx;
  /* position: relative;
  top:8rpx; */
  margin:0 10rpx 0 0;
}
.select_time_title,.select_time{
  line-height:30px;
  padding:0 20rpx;
}
.select_time{
  background: #fff;
}
.header{
  height:50px;
  background: #fff;
  text-align: center;
  border-top:10px solid #F1F1F1;
  color:#979797;
  font-size:14px;
  white-space: nowrap;
}
.header .header_view{
  width:20%;
  height:50px;
  padding: 6px 0;
  display: inline-block;
}
.header .active{
  background: #1ABB85;
  color:#fff;
}
.search{
  width:14px;
}
.search_wrap{
  width:30px;
  background:#fff;
  display:flex;
  height:40px;
  align-items:center;
  justify-content:center;
  border-top-left-radius : 4px;
  border-bottom-left-radius : 4px;  
}
.search_subject_wrap{
    height: 60px;
    box-sizing: border-box;
  border-radius: 10px;
  padding: 10px 0 10px 10px;
}
.search_subject{
  width: calc(100% - 40px);
  background: #FFFFFF;
  height:40px;
  border-top-right-radius : 4px;
  border-bottom-right-radius : 4px;
}
.subjects>view>view{
  line-height:40px;
  padding: 0 0 0 6px;
}
.big_subject{
  width:40%;
  overflow: auto;
}
.big_subject view{
  color:#a5a5a5;
}
.big_subject>.active{
  background: #fff;
  color:#5DBC84;
  border-left:3px solid #5DBC84;
}
.small_subject{
  /*width:60%;*/
  background: #fff;
  overflow: auto;
}
.small_subject view{
  color:#333333;
}
.small_subject .subject_con view navigator{
  background: #fff;
  border-bottom: 1px solid #EEEEEE;
  line-height:40px;
  padding: 0 6px;
  display:flex;
  align-items:center;
  align-content:space-around;
  justify-content:space-between;
}
.subject_sel{
  width:10px;
}
.appoint_info_wrap{
    width:100%;
    height: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    position: fixed;
    z-index: 1;
    background: #fff;
    top:0;
    right:0;
    bottom:0;
    left:0;
}
.confirm_wrap{
    padding: 10px;
}
.appoint_con{
    padding: 20rpx 0 0 0;
}
.appoint_con>view{
    padding: 0 20rpx;
}
.confirm{
    height:80rpx;
    line-height:80rpx;
    background: #1abb85;
    color:#fff;
}