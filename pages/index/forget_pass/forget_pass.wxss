.avatar{
  width:100rpx;
}
.mine_items{
  border-bottom:4rpx solid #F2F2F2;
  color:#272727;
}
.mine_items text{
  color:#666666;
}
.mine_item{
  border-top:2rpx solid #F2F2F2;
  min-height:80rpx;
  justify-content:space-between;
  padding:20rpx 30rpx;
  position: relative;
}
.last_mine_item{
  border-bottom:2rpx solid #F2F2F2;  
}
.mine_icon{
  width:50rpx;
  margin:0 30rpx 0 0;
}
.nav{
  width:20rpx;
}
.id_card{
  border-bottom:20rpx solid #F2F2F2;
}
.mine_item view:first-child{
  width:160rpx;
}
.info_right{
  width:calc(100% - 160rpx);
}
.get_code{
  position:absolute;
  z-index: 1;
  right:4rpx;
  top:11rpx;
}
.agree{
  padding: 10rpx 0 0 30rpx;
}