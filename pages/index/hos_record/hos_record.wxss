page{
    background: #f5f5f5;
    line-height: 80rpx;
}
.header{
  background: #fff;
}
.header>view{
    padding: 0 18rpx;
}
.info_header{
    border-bottom:1px solid #f2f2f2;
}
.set_time{
  float:right;
  padding:0 10rpx;
}
.flex{
  justify-content: space-between;
}
.report_item{
    background: #fff;
    margin-bottom:20rpx;
}
.report_item>view{
    padding: 0 20rpx;
}
.title{
  padding: 0 18rpx;
  line-height:80rpx;
}
.nav{
  width:16rpx;
}
.flex .title_left{
  flex:none;
}
.text-right{
  text-align: right;
}
.report_info view{
  display: flex;
  align-items: center;
  line-height:80rpx;
}
.exma_icon{
  width:30rpx;
  margin:0 10rpx 0 0;
}
.triangle{
  width:30rpx;
}
.small{
    font-size:28rpx;
}