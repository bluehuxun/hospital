page{
	background:#f2f2f2;
    color:#363636;
    padding: 30rpx;
	box-sizing: border-box;
}
.list>view{
    line-height: 80rpx;
	background: #fff;
    margin:0 0 30rpx 0;
    box-shadow:0 0 40rpx #ccc;
    border-radius: 10rpx;
}
.list>view>view{
    display:flex;
    align-items:center;
    border-bottom:1px solid #f3f3f3;
    padding:10rpx 20rpx;
}
.icon{
    width:34rpx;
    margin:0 20rpx 0 0;
}
.list .pull-left{
	min-width:200rpx;
	color:#a7a7a7;
}
.list .pull-right{
    padding: 0 0 0 20rpx;
}
.gray_title{
	line-height:30px;
	padding:0 0 0 20px;
	background:#f5f5f5;
}
.info_list>view{
	line-height:30px;
	padding:0 0 0 20px;
}
.info_left{
	width:2rem;
}
.info_right{
	/*width:70%;*/
    line-height: 30px;
	padding: 0 8px;
}
.info_right .pull-left{
	width:80px;
}
.info_right .pull-right{
	width:calc(100% - 80px);
}
.login{
	width: calc(100% - 20px);
    margin: 0 auto 0 auto;
	color:#fff;
	background:#108ee9;
}
.icon_wrap{
	width:17px;
	text-align:center;
	height: 30px;
    line-height: 30px;
}
image{
	vertical-align:middle;
}
.login_wrap{
	padding:5px 0!important;
}
.forget_pass,.login_wrap,.to_register{
	padding:0 5px!important;
}
.forget_pass,.to_register{
	padding:5px 10px!important;
}
.forget_pass{
	line-height:30px;
}
.info_right>view{
	border-bottom :1px solid #f2f2f2;
}
.btn_wrap{
	padding :4px 4px 4px 0;
}
.avator_wrap{
	width:30%;
	height :124px;
}
.avator_wrap image{
	display :block;

	width:100%;
	height :100%;
}
.avator{
	width:80px;
	height :112px;
}
.bottom{
	width:100%;
	height:56px;
	bottom:0;
	left:0;
	font-size:24rpx;
	padding :10rpx 0 0 0;
	border-top:1px solid #f2f2f2;
}
.bottom_icon{
	width:40px;
	height:40px!important;
}
.bottom>view>view{
	padding :0 0 10rpx 0;
}
.signed{
    color:#666;
}
.break_signed{
    color:#fe3232;
}